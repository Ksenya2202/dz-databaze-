SELECT title, duration 
FROM Tracks 
ORDER BY duration DESC 
LIMIT 1;

SELECT title 
FROM Tracks 
WHERE duration >= 210;

SELECT title 
FROM Compilations 
WHERE release_year BETWEEN 2018 AND 2020;

SELECT name 
FROM Artists 
WHERE name NOT LIKE '% %';

SELECT title 
FROM Tracks 
WHERE LOWER(title) LIKE '%мой%' OR LOWER(title) LIKE '%my%';
